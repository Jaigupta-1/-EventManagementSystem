{
    "compilerOptions": {
      "module": "commonjs",
      "target": "ES2016",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "sourceMap": true,
      "strictNullChecks": false,
      "noUnusedLocals": true,
      "pretty": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "outDir": "./dist",
      "forceConsistentCasingInFileNames": false,
      "typeRoots": ["./node_modules/@types","src/models/types"]
    },
    "exclude": [
      "../node_modules",
      "../src",
    ]
  }
  